{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@date-io/date-fns": "^2.11.0",
        "@emotion/react": "^11.7.0",
        "@emotion/styled": "^11.6.0",
        "@fontsource/open-sans": "^4.5.8",
        "@fontsource/plus-jakarta-sans": "^4.5.4",
        "@mui/icons-material": "^5.2.1",
        "@mui/lab": "^5.0.0-alpha.60",
        "@mui/material": "^5.4.3",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^11.2.7",
        "@types/jest": "^26.0.24",
        "@types/node": "^12.20.37",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "date-fns": "^2.23.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jest-dom": "^4.0.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-testing-library": "^5.0.3",
        "prettier": "^2.5.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.1.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.5.2",
        "user-event": "^4.0.0",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "start:node17": "react-scripts --openssl-legacy-provider start",
        "build": "react-scripts build",
        "build:node17": "react-scripts --openssl-legacy-provider build",
        "test": "react-scripts test",
        "test:unit": "cross-env DEBUG_PRINT_LIMIT=100000 react-scripts test --coverage --watchAll=false",
        "test:e2e-ci": "npx cypress run --browser chrome --headless --config-file cypress/config/pipeline.json",
        "eject": "react-scripts eject",
        "lint": "eslint . --ignore-path .gitignore --ext js,ts,tsx",
        "lint:fix": "eslint . --fix",
        "prepare": "husky install",
        "cypress:open": "cypress open"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "jestConfig": {
        "resetMocks": false
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:6000",
    "devDependencies": {
        "@testing-library/dom": "^8.11.1",
        "@testing-library/user-event": "^13.5.0",
        "cross-env": "^7.0.3",
        "eslint-config-react-app": "^7.0.0",
        "fetch-mock": "^9.11.0",
        "husky": "^7.0.0",
        "jest-fetch-mock": "^3.0.3",
        "jest-watch-typeahead": "^0.6.5",
        "lint-staged": "^12.1.5"
    },
    "overrides": {
        "nth-check": "2.0.1",
        "postcss": "8.4.5",
        "follow-redirects": "1.14.8",
        "async": "3.2.2"
    }
}
